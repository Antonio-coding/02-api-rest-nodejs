    "coffeescript": "^2.7.0",
    "eslint": "^8.47.0",
    "supertest": "^6.3.3",
    "ts-node": "^10.9.1",
    "tsup": "^7.2.0",
    "tsx": "^3.12.7",
    "typescript": "^5.1.6",
    "vitest": "^0.34.3"
  },
  "dependencies": {
    "@fastify/cookie": "^9.0.4",
    "dotenv": "^16.3.1",
    "fastify": "^4.21.0",
